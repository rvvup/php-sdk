{
    "name": "rvvup/sdk",
    "description": "Rvvup payments SDK",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "suggest": {
        "guzzlehttp/guzzle": ">=6"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^9 || ^10 || ^11",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Rvvup\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rvvup\\Sdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
